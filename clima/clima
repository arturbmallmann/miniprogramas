                <script type="text/javascript">

                    var photos=new Array()
                    var photoslink=new Array()
                    var which=0
                    var link_img = "http://img0.cptec.inpe.br/~rgrafico/portal_tempo/bandas/";
                    var link_bol = "http://tempo1.cptec.inpe.br/~rtempo/txt/";

                    var CO = ["coe_reg","centroeste","","tx","ti"];
                    var S = ["sul_reg","sul","","tx","ti"];
                    var SE = ["sud_reg","sudeste","","tx","ti"];
                    var N = ["nor_reg","norte","","tx","ti"];
                    var NE = ["nde_reg","nordeste","","tx","ti"];



                    for(i = 0; i<4; i++)
                        photos[i]=link_img+S[0]+S[2]+(i+1)+".jpg"

                    function aa(v){
                        valor = v;

                        for(i = 0; i<4; i    ++)
                            photos[i]=link_img+S[0]+S[v]+(i+1)+".jpg"

                        var preloadedimages=new Array()
                        for (i=0;i<photos.length;i++){
                            preloadedimages[i]=new Image()
                            preloadedimages[i].src=photos[i]
                        }
                        document.images.photoslider.src=photos[which]
                        //Update()
                    }
                    //define images. You can have as many as you want:

                    //Specify whether images should be linked or not (1=linked)
                    var linkornot=0

                    //Set corresponding URLs for above images. Define ONLY if variable linkornot equals "1"
                    for(i = 0; i<4; i++)
                        photoslink[i]=""

                    //do NOT edit pass this line

                    var preloadedimages=new Array()
                    for (i=0;i<photos.length;i++){
                        preloadedimages[i]=new Image()
                        preloadedimages[i].src=photos[i]
                    }

                    function Update(){

                        //var regiao = Dados();
                        getobject("NUM_IMG").innerHTML = which+1;
                        getobject("TOTAL_IMG").innerHTML = photos.length;
                        document.getElementById('texto').src= link_bol+S[1]+".shtml";

                    }

                    function getobject(obj){
                        if (document.getElementById)
                            return document.getElementById(obj)
                        else if (document.all)
                            return document.all[obj]
                    }

                    function applyeffect(){
                        if (document.all && photoslider.filters){
                            photoslider.filters.revealTrans.Transition=Math.floor(6)
                            photoslider.filters.revealTrans.stop()
                            photoslider.filters.revealTrans.apply()
                        }
                    }

                    function playeffect(){
                        if (document.all && photoslider.filters)
                            photoslider.filters.revealTrans.play()
                    }

                    function keeptrack(){
                        window.status="Image "+(which+1)+" of "+photos.length

                    }

                    function backward(){
                        if (which>0){
                            which--
                            applyeffect()
                            document.images.photoslider.src=photos[which]
                            playeffect()
                            keeptrack()
                            Update()
                        }
                    }

                    function forward(){
                        if (which<photos.length-1){
                            which++
                            applyeffect()
                            document.images.photoslider.src=photos[which]
                            playeffect()
                            keeptrack()
                            Update()
                        }
                    }

                    function transport(){
                        window.location=photoslink[which]
                    }

                    function MM_jumpMenu(targ,selObj,restore){ //v3.0
                        eval(targ+".location='"+selObj.options[selObj.selectedIndex].value+"'");
                        if (restore) selObj.selectedIndex=0;
                    }

                    function go(pagina) {
                        window.open(pagina,'Legenda','toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=no,resizable=no,menubar=no,width=575,height=485')
                    }
                    function janela(link,wid,hei) {
                        window.open(link,'new_window','toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=no,menubar=no,width='+wid+',height='+hei)
                    }

                    function setCookie(name, value, cidade) {


                        var days = 364;
                        var expireDate = new Date()
                        var path = "/cidades";
                        var expstring=(typeof days!="undefined")? expireDate.setDate(expireDate.getDate()+parseInt(days)) : expireDate.setDate(expireDate.getDate()-5)
                        var curCookie = name + "=" + escape(value) +";path="+path+"; expires="+expireDate.toGMTString();
                        document.cookie = curCookie;
                        alert(cidade+ " agora é a sua cidade Padrão");
                    }
                    function Chamar() {

                        window.onLoad=Update();
                        window.onLoad=pestanasStart();
                        window.onLoad=WebRadio();


                    }
                    function limpar(){
                        document.regiaoForm2.name.value="";

                    }
                    function mudabox(val){

                        if (val == 1){
                            document.getElementById('box1').style.display="block";
                            document.getElementById('box2').style.display="none";

                        }
                        else{
                            document.getElementById('box1').style.display="none";
                            document.getElementById('box2').style.display="block";

                        }
                    }

                </script>
