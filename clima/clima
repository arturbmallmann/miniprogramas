                    $(document).ready(function(){
                        //   $('.dropMenu').before(' <div class=topItem> Outros Revelar<img src=../imgs/pixel.gif width=30px></div>');
                        $('.dropMenu').css('display', 'none');
                        $('.mainDiv .topItem').addClass('active');
                        $('.mainDiv .topItem').click(function() {

                            // $(".topItem").removeClass('disable');
                            if ($(this).attr('class') == "topItem disable") {
                                //  alert("Desativo......:"+$(this).attr('class'))
                                $(this).next().slideToggle('slow');
                                $(this).removeClass('disable');
                                return false; // Don't click through
                            } else {
                                // alert("Ativo.........:"+$(this).attr('class'))
                                $(this).next().slideToggle('slow')
                                .siblings('.dropMenu div:visible').slideToggle('fast');
                            }
                            //  $(".topItem").removeClass('active'); //Remove class of 'active' on all lists
                            $(this).removeClass('active');
                            $(this).addClass('disable');//add class of 'active' on this list only
                            return false;
                        });

                        // $(this).toggleText('Revelar','Esconder')
                        // .siblings('div').next('.dropMenu:visible').prev()
                        // .toggleText('Revelar','Esconder')

                    })
